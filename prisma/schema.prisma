generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  email    String @unique(map: "email") @db.VarChar(255)
  password String @db.VarChar(255)
}

model ordedrs {
  id       Int     @id @default(autoincrement())
  name     String? @db.VarChar(255)
  quantity Int?
}
